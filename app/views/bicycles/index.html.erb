<h1>My Bike Inventory</h1>
<section>
	<% if defined? @bicycles %>
  	<% @bicycles.each do |bicycle| %>
    	<article>
      	<section>
        	<h3><%= link_to bicycle.model, bicycle_path(bicycle) %></h3>
        
      	</section>
    	</article>
  	<% end %>
  <% end %>

  <%= link_to "Add a new bicycle", new_bicycle_path %>
	
</section>
